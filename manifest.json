{
    "manifest_version": 2,
    "name": "OCWCandy",
    "version": "0.2.1",
    "description": "Correct for page style for two windows side by side",
    "author": "Pavel-Vlad Mateescu",
    "browser_action": {
        "default_icon": {
          "16": "images/icon16.png",
          "24": "images/icon24.png",
          "32": "images/icon32.png"
        },
        "default_title": "OCW Candy",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["https://ocw.cs.pub.ro/courses/*/laboratoare/*", "https://ocw.cs.pub.ro/courses/*/teme/*"],
        "js": ["jquery.js"]
    }] 
}
